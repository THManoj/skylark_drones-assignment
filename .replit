run = "streamlit run app.py"
modules = ["python-3.10"]

[nix]
channel = "stable-23_11"

[unitTest]
language = "python3"

[languages.python3]
pattern = "**/*.py"

[languages.python3.languageServer]
start = "pylsp"

[debugger]
support = true

[debugger.interactive]
transport = "localhost:0"
startCommand = ["dap-python", "app.py"]
